{
	"name": "dion-build-index",
	"type": "module",
	"version": "0.2.1",
	"bin": {
		"dion-build-index": "./.dist/main.js"
	},
	"exports": {
		"./*": "./.dist/*"
	},
	"files": [
		".dist"
	],
	"scripts": {
		"check-types": "bunx tsc -p tsconfig.json",
		"build": "bun run ./build.ts",
		"lint": "bunx biome check",
		"format": "bunx biome format --write"
	},
	"dependencies": {
		"dion-repo-utils": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@types/bun": "catalog:",
		"dion-runtime-types": "catalog:",
		"dion-runtime-configs": "workspace:*",
		"typescript": "catalog:"
	}
}
